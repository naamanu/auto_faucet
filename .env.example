# Circle API Key (get from https://console.circle.com)
CIRCLE_API_KEY=TEST_API_KEY:your_api_key_here

# Wallet addresses (comma-separated, no spaces)
ADDRESSES=YourSolanaAddress1,YourSolanaAddress2

# Blockchain (SOL-DEVNET for Solana testnet)
BLOCKCHAIN=SOL-DEVNET

# Claim interval in hours (default: 2)
INTERVAL_HOURS=2
